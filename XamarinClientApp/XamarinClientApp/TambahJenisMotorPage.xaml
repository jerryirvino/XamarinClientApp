<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XamarinClientApp.TambahJenisMotorPage">
  
  <StackLayout>
    <Label Text="Masukan Nama Merk Motor :" />
    <Entry x:Name="txtNamaMerkMotor" Placeholder="Masukan Nama Merk Motor"/>
    <Label Text="Masukan Nama Jenis Motor :" />
    <Entry x:Name="txtNamaJenisMotor" Placeholder="Masukan Nama Jenis Motor"/>
    <Button x:Name="btnTambahJenisMotor" Text="Tambah" />
  </StackLayout>
</ContentPage>